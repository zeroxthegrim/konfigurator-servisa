   }\r\n        setTotal(total);\r\n    }, [basePrice, usingCoupon, tax]);\r\n\r\n\r\n    return (\r\n        <Context.Provider value={{\r\n            manufacturer,\r\n            checkedServices,\r\n            addManufacturer,\r\n            addService,\r\n            removeService,\r\n            getBasePrice,\r\n            savings,\r\n            total,\r\n            basePrice,\r\n            usingCoupon,\r\n            toggleCoupon,\r\n            name,\r\n            setName,\r\n            email,\r\n            setEmail,\r\n            telNum,\r\n            setTelNum,\r\n            note,\r\n            setNote,\r\n            clearData\r\n        }}>\r\n            {children}\r\n        </Context.Provider>\r\n    )\r\n}\r\n\r\nexport { ContextProvider, Context }","C:\\Users\\Niksa\\Desktop\\konfigurator-servisa\\src\\components\\Modals\\Modal4.js",[],"C:\\Users\\Niksa\\Desktop\\konfigurator-servisa\\src\\components\\Modals\\Modal5.js",[],{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":25,"column":12,"nodeType":"60","messageId":"61","endLine":25,"endColumn":19},{"ruleId":"62","severity":1,"message":"63","line":41,"column":33,"nodeType":"64","messageId":"65","endLine":41,"endColumn":34,"suggestions":"66"},{"ruleId":"62","severity":1,"message":"63","line":41,"column":58,"nodeType":"64","messageId":"65","endLine":41,"endColumn":59,"suggestions":"67"},{"ruleId":"58","severity":1,"message":"68","line":13,"column":17,"nodeType":"60","messageId":"61","endLine":13,"endColumn":23},"no-native-reassign",["69"],"no-negated-in-lhs",["70"],"no-unused-vars","'checked' is assigned a value but never used.","Identifier","unusedVar","no-useless-escape","Unnecessary escape character: \\[.","Literal","unnecessaryEscape",["71","72"],["73","74"],"'setTax' is assigned a value but never used.","no-global-assign","no-unsafe-negation",{"messageId":"75","fix":"76","desc":"77"},{"messageId":"78","fix":"79","desc":"